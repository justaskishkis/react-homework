{
	"name": "react-homework",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "npm-run-all -p watch-css start-js",
		"start-js": "react-scripts start",
		"build": "npm-run-all build-css build-js",
		"build-js": "react-scripts build",
		"now-build": "npm run build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"build-css": "stylus -u autoprefixer-stylus -c src/assets/stylus/ --out src/assets/css",
		"watch-css": "npm run build-css && stylus -u autoprefixer-stylus -c -w src/assets/stylus/ --out src/assets/css"
	},
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.4.0",
		"autoprefixer-stylus": "1.0.0",
		"connected-react-router": "^6.7.0",
		"history": "^4.10.1",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-beautiful-dnd": "12.2.0",
		"react-dom": "^16.12.0",
		"react-redux": "^7.1.3",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"redux": "^4.0.5",
		"redux-devtools-extension": "^2.13.8",
		"redux-saga": "^1.1.3",
		"stylus": "^0.54.7",
		"typesafe-actions": "^4.4.2"
	},
	"devDependencies": {
		"@types/history": "^4.7.5",
		"@types/jest": "^25.1.2",
		"@types/node": "^13.7.1",
		"@types/react": "^16.9.19",
		"@types/react-beautiful-dnd": "^12.1.1",
		"@types/react-dom": "^16.9.5",
		"@types/react-redux": "^7.1.7",
		"@types/react-router-dom": "^5.1.3",
		"@types/redux": "^3.6.0",
		"css-loader": "^3.4.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1",
		"react-scripts": "^3.4.0",
		"stylus-loader": "^3.0.2",
		"tslint": "~6.0.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "~3.7.5"
	},
	"browserslist": {
		"development": [
			"last 2 chrome versions",
			"last 2 firefox versions",
			"last 2 edge versions"
		],
		"production": [
			">1%",
			"Firefox ESR",
			"not ie <= 11",
			"not op_mini all"
		]
	}
}
